<p id="notice"><%= notice %></p>

<h1>Listing Dealer Securities</h1>

<table>
  <thead>
    <tr>
      <th>Dealer</th>
      <th>Security check</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dealer_securities.each do |dealer_security| %>
      <tr>
        <td><%= dealer_security.dealer_id %></td>
        <td><%= dealer_security.security_check_id %></td>
        <td><%= link_to 'Show', dealer_security %></td>
        <td><%= link_to 'Edit', edit_dealer_security_path(dealer_security) %></td>
        <td><%= link_to 'Destroy', dealer_security, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dealer security', new_dealer_security_path %>
