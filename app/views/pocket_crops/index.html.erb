<p id="notice"><%= notice %></p>

<h1>Listing Pocket Crops</h1>

<table>
  <thead>
    <tr>
      <th>Pocket</th>
      <th>Crop</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pocket_crops.each do |pocket_crop| %>
      <tr>
        <td><%= pocket_crop.pocket_id %></td>
        <td><%= pocket_crop.crop_id %></td>
        <td><%= link_to 'Show', pocket_crop %></td>
        <td><%= link_to 'Edit', edit_pocket_crop_path(pocket_crop) %></td>
        <td><%= link_to 'Destroy', pocket_crop, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pocket crop', new_pocket_crop_path %>
