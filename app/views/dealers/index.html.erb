<p id="notice"><%= notice %></p>

<h1>Listing Dealers</h1>

<table>
  <thead>
    <tr>
      <th>State</th>
      <th>User</th>
      <th>Pocket</th>
      <th>Name</th>
      <th>Address</th>
      <th>Atpost</th>
      <th>Pin code</th>
      <th>Taluka</th>
      <th>District</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dealers.each do |dealer| %>
      <tr>
        <td><%= dealer.state_id %></td>
        <td><%= dealer.user_id %></td>
        <td><%= dealer.pocket_id %></td>
        <td><%= dealer.name %></td>
        <td><%= dealer.address %></td>
        <td><%= dealer.atpost %></td>
        <td><%= dealer.pin_code %></td>
        <td><%= dealer.taluka_id %></td>
        <td><%= dealer.district_id %></td>
        <td><%= link_to 'Show', dealer %></td>
        <td><%= link_to 'Edit', edit_dealer_path(dealer) %></td>
        <td><%= link_to 'Destroy', dealer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dealer', new_dealer_path %>
