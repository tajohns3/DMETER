<p id="notice"><%= notice %></p>

<h1>Listing Product Prescriptions</h1>

<table>
  <thead>
    <tr>
      <th>Dealer</th>
      <th>Farmer</th>
      <th>Crop</th>
      <th>Condition</th>
      <th>Crop growth</th>
      <th>Crop cond</th>
      <th>Acreage</th>
      <th>Prescribe</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_prescriptions.each do |product_prescription| %>
      <tr>
        <td><%= product_prescription.dealer_id %></td>
        <td><%= product_prescription.farmer_id %></td>
        <td><%= product_prescription.crop_id %></td>
        <td><%= product_prescription.condition %></td>
        <td><%= product_prescription.crop_growth %></td>
        <td><%= product_prescription.crop_cond %></td>
        <td><%= product_prescription.acreage %></td>
        <td><%= product_prescription.prescribe %></td>
        <td><%= link_to 'Show', product_prescription %></td>
        <td><%= link_to 'Edit', edit_product_prescription_path(product_prescription) %></td>
        <td><%= link_to 'Destroy', product_prescription, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product prescription', new_product_prescription_path %>
