<p id="notice"><%= notice %></p>

<h1>Listing Security Checks</h1>

<table>
  <thead>
    <tr>
      <th>Bank name</th>
      <th>Check number</th>
      <th>Bank account</th>
      <th>Amount</th>
      <th>Issue date</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @security_checks.each do |security_check| %>
      <tr>
        <td><%= security_check.bank_name %></td>
        <td><%= security_check.check_number %></td>
        <td><%= security_check.bank_account %></td>
        <td><%= security_check.amount %></td>
        <td><%= security_check.issue_date %></td>
        <td><%= security_check.status %></td>
        <td><%= link_to 'Show', security_check %></td>
        <td><%= link_to 'Edit', edit_security_check_path(security_check) %></td>
        <td><%= link_to 'Destroy', security_check, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Security check', new_security_check_path %>
