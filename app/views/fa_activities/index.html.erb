<p id="notice"><%= notice %></p>

<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta name="description" content="">
  <meta name="author" content="">
  <title>
  </title>
  <link href="https://netdna.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css"
        rel="stylesheet">
</head>

<body>
<div class="container">
  <div class="panel panel-default">
    <div class="panel-heading ">
      <h1 class="panel-title ">
        <b> FA Activities</b>
      </h1>
    </div>
    <div class="panel-body">
      <div class="row">
        <div class="col-md-12">
          <table class="table table-bordered">
            <tbody>
            <tr>
              <th class="text-center">
                Field Assistant
              </th>
              <th class="text-center">
                State
              </th>
              <th class="text-center">
                User
              </th>
              <th class="text-center">
                Pocket
              </th>
              <th class="text-center">
                Date
              </th>
              <th class="text-center">
                Comments
              </th>
              <td class="text-center">
                <%= link_to 'New FA Activity', new_fa_activity_path, 'data-no-turbolink' => true, class: "btn btn-success",:style=>'color:#FFFFFF'%>
              </td>
            </tr>
            <tr>
              <% @fa_activities.each do |fa_activity| %>
                  <td class="text-center">
                    <%= fa_activity.field_assistant_id %>
                  </td>
                  <td class="text-center">
                    <%= fa_activity.state_id %>
                  </td>
                  <td class="text-center">
                    <%= fa_activity.user_id %>
                  </td>
                  <td class="text-center">
                    <%= fa_activity.pocket_id %>
                  </td>
                  <td class="text-center">
                    <%= fa_activity.date %>
                  </td>
                  <td class="text-center">
                    <%= fa_activity.comment %>
                  </td>
                  <td class="text-center">
                    <%= link_to 'View FA activities', pre_demo, class: "btn btn-primary",:style=>'color:#FFFFFF' %>
                  </td>
                  </tr>
              <% end %>
              </tbody>
          </table>
        </div>
      </div>
      <div class="row">
        <div class="col-md-4">
        </div>
        <div class="col-md-4">
        </div>
        <div class="col-md-4">
        </div>
      </div>
    </div>
  </div>
</div>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"
>
</script>
<script src="https://netdna.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js"
>
</script>
</body>
</html>
