<div class="nested-fields">
  <div class="field">
    <%= f.label :product_id %><br>
    <%= f.collection_select :product_id, Product.all, :id,:name %>
  </div>

  <div class="field">
    <%= f.label :quantity %><br>
    <%= f.text_field :quantity%>
  </div>
  <%=link_to_remove_association 'Remove product', f %>
</div>