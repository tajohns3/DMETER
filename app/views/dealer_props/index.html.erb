<p id="notice"><%= notice %></p>

<h1>Listing Dealer Props</h1>

<table>
  <thead>
    <tr>
      <th>Dealer</th>
      <th>Proprietor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dealer_props.each do |dealer_prop| %>
      <tr>
        <td><%= dealer_prop.dealer_id %></td>
        <td><%= dealer_prop.proprietor_id %></td>
        <td><%= link_to 'Show', dealer_prop %></td>
        <td><%= link_to 'Edit', edit_dealer_prop_path(dealer_prop) %></td>
        <td><%= link_to 'Destroy', dealer_prop, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dealer prop', new_dealer_prop_path %>
