<p id="notice"><%= notice %></p>

<h1>Listing Demonstration Attachments</h1>

<table>
  <thead>
    <tr>
      <th>Demonstration</th>
      <th>Avatar</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @demonstration_attachments.each do |demonstration_attachment| %>
      <tr>
        <td><%= demonstration_attachment.demonstration_id %></td>
        <td><%= demonstration_attachment.avatar %></td>
        <td><%= link_to 'Show', demonstration_attachment %></td>
        <td><%= link_to 'Edit', edit_demonstration_attachment_path(demonstration_attachment) %></td>
        <td><%= link_to 'Destroy', demonstration_attachment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Demonstration attachment', new_demonstration_attachment_path %>
